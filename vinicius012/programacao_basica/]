import java.util.Scanner;

public class Questao08 {
    public static void main(String[] args){
        Scanner s;
        s = new Scanner(System.in);
      
        System.out.print("Digite o lado do cubo: ");
        double a = s.nextDouble();

        System.out.print("Digite a altura do paralelepipedo: ");
        double b = s.nextDouble();

        System.out.print("Digite o comprimento do paralelepipedo: ");
        double c = s.nextDouble();
 
        System.out.print("Digite a profundidade do paralelepipedo: ");
        double d = s.nextDouble();

        System.out.print("Digite a base do cone: ");
        double e = s.nextDouble();

        System.out.print("Digite a altura do cone: ");
        double f = s.nextDouble();

        System.out.print("Digite o raio da esfera: "); 
        double g = s.nextDouble();

        double x = a * a * a;
        double y = b * c * d;
        double z = 1.0 / 3.0 * 3.141592 *  e * e * f;
        double w = 4.0 / 3.0 * 3.141592 * g * g * g;

        System.out.println(" O volume do cubo e " + x);
        System.out.println(" O volume do paralelepipedo e " + y);
        System.out.println(" O volume do cone e " + z);
        System.out.println(" O volume da esfera e " + w);

    }
}
   
